PROJECTS = DCLMDO70.bpl RCLMDO70.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DCLMDO70.bpl: DCLMDO70.dpk
  $(DCC)

RCLMDO70.bpl: RCLMDO70.dpk
  $(DCC)


