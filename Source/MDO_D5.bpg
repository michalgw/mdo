MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = DCLMDO50.bpl RCLMDO50.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DCLMDO50.bpl: DCLMDO50.dpk
  $(DCC)

RCLMDO50.bpl: RCLMDO50.dpk
  $(DCC)


