MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = DCLMDO60.bpl RCLMDO60.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DCLMDO60.bpl: DCLMDO60.dpk
  $(DCC)

RCLMDO60.bpl: RCLMDO60.dpk
  $(DCC)


